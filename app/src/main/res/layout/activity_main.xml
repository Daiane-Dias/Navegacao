<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity"
        android:orientation="vertical"
        tools:ignore="RedundantNamespace">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/nosso_mapa" />

        <Button
            android:id="@+id/verimagem"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="ExibirMapa"
            android:text="@string/ver_imagem"
            tools:ignore="OnClick,UsingOnClickInXml" />

        <Button
            android:id="@+id/vervetor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="ExibirMapa"
            android:text="@string/ver_vetor"
            tools:ignore="OnClick,UsingOnClickInXml" />

        <Button
            android:id="@+id/verrua"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="ExibirMapa"
            android:text="@string/ver_rua"
            tools:ignore="OnClick,UsingOnClickInXml" />
        <com.esri.arcgisruntime.mapping.view.MapView
            android:id="@+id/mapView"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </com.esri.arcgisruntime.mapping.view.MapView>
    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>